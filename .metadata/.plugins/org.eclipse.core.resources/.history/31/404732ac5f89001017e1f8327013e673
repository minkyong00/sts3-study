package springdi_xml_exam.bean;

public class Score {

	private int kor;
	private int eng;
	private int math;
	private int sum;
	private int avg;
	
	public Score() {
	}

	public Score(int kor, int eng, int math, int sum, int avg) {
		this.kor = kor;
		this.eng = eng;
		this.math = math;
		this.sum = sum;
		this.avg = avg;
	}
	
	

	
	
}
