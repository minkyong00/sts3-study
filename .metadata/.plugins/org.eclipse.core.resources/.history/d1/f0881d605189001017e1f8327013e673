package springdi_xml.bean;

import java.util.List;
import java.util.Map;
import java.util.Properties;
import java.util.Set;

public class TV {

	private Speaker speaker;
	private String name;
	private List<String> parts;
	private Set<String> colors;
	private Map<String, String> yearPrice;
	private Properties spec;
	
	public TV() {
		System.out.println("TV 객체가 생성됨!");
	}

	public TV(Speaker speaker, String name, List<String> parts, Set<String> colors, 
			Map<String, String> yearPrice, Properties spec) {
		this.speaker = speaker;
		this.name = name;
		this.parts = parts;
		this.colors = colors;
		this.yearPrice = yearPrice;
		this.spec = spec;
	}

	
	
}
